#: ../adduser:125 ../deluser:99
msgid "No options allowed after names.\n"
msgstr "Нельзя указывать параметры после имён.\n"

#: ../adduser:144
msgid "--ingroup requires an argument.\n"
msgstr "Для опции --ingroup требуется агрумент.\n"

#: ../adduser:147
msgid "--home requires an argument.\n"
msgstr "Для опции --home требуется аргумент.\n"

#: ../adduser:149
msgid "Warning: The home dir you specified already exists.\n"
msgstr "Предупреждение: Заданный вами домашний каталог уже существует.\n"

#: ../adduser:151
msgid "The home dir must be an absolute path.\n"
msgstr "Домашний каталог должен задаваться в виде абсолютного пути.\n"

#: ../adduser:154
msgid "--gecos requires an argument.\n"
msgstr "Для опции --gecos требуется аргумент.\n"

#: ../adduser:157
msgid "--shell requires an argument.\n"
msgstr "Для опции --shell требуется аргумент.\n"

#: ../adduser:166
msgid "--uid requires a numeric argument.\n"
msgstr "Для опции --uid требуется числовой аргумент.\n"

