#: ../../deluser:100
msgid "No options allowed after names.\n"
msgstr "Keine Optionen nach dem Namen erlaubt.\n"

#: ../../deluser:123
msgid "--conf requires an argument.\n"
msgstr "--conf setzt ein Argument voraus.\n"

#: ../../deluser:142
#, c-format
msgid "Unknown argument `%s'.\n"
msgstr "Unbekanntes Argument `%s'\n"

#: ../../deluser:161
msgid "Enter a groupname to remove: "
msgstr "Geben Sie den zu entfernenen Gruppennamen ein: "

#: ../../deluser:165
msgid "Enter a username to remove: "
msgstr "Geben Sie den zu entfernenen Benutzernamen ein: "

#: ../../deluser:171
msgid "I need a name to remove.\n"
msgstr "Ich benötige einen Namen, der entfernt werden soll.\n"

#: ../../deluser:172
msgid "No more than two names.\n"
msgstr "Mehr als zwei Namen können nicht angegeben werden.\n"

#: ../../deluser:176
msgid "Specify only one name in this mode.\n"
msgstr "In diesem Modus können Sie nur einen Namen angeben.\n"

#: ../../deluser:223 ../../deluser:267 ../../deluser:282 ../../deluser:286
#, c-format
msgid "`%s' does not exist!\n"
msgstr "`%s' existiert nicht!\n"

#: ../../deluser:228
msgid "Searching for files to backup/remove...\n"
msgstr "Suche Dateien, die gesichert/gelöscht werden müssen...\n"

#: ../../deluser:244
msgid "Backuping files to be removed...\n"
msgstr "Sichere die zu löschenden Dateien...\n"

#: ../../deluser:250
msgid "Removing files...\n"
msgstr "Lösche Dateien...\n"

#: ../../deluser:255
#, c-format
msgid "Removing user %s...\n"
msgstr "Entferne Benutzer %s...\n"

#: ../../deluser:261 ../../deluser:276 ../../deluser:314
msgid "done.\n"
msgstr "Fertig.\n"

#: ../../deluser:271
#, c-format
msgid "There are users having `%s' as primary group!\n"
msgstr "Es gibt Benutzer, die %s als primäre Gruppe haben!\n"

#: ../../deluser:274
#, c-format
msgid "Removing group %s...\n"
msgstr "Entferne Gruppe %s...\n"

#: ../../deluser:290
msgid "You may not remove the user from his/her primary group.\n"
msgstr "Sie können den Benutzer nicht aus seiner primären Gruppe entfernen.\n"

#: ../../deluser:309
msgid "%s is not a member of group %s.\n"
msgstr "%s ist kein Mitglied in der Gruppe %s.\n"

#: ../../deluser:312
#, c-format
msgid "Removing user %s from group %s...\n"
msgstr "Entferne Benutzer %s aus Gruppe %s...\n"

#: ../../deluser:322 ../../deluser:342
msgid "removing user and groups from the system. Version:"
msgstr "entfernt Benutzer und Gruppen vom System. Version:"

#: ../../deluser:367
#, c-format
msgid "Global configuration is in the file %s.\n"
msgstr "Die globale Konfiguration ist in der Datei %s.\n"

#: ../../deluser:388
#, c-format
msgid "%s: %s doesn't exist.  Using defaults.\n"
msgstr "%s: %s existiert nicht. Benutze Standardwerte.\n"

#: ../../deluser:398
#, c-format
msgid "Couldn't parse %s:%s.\n"
msgstr "Konnte %s:%s nicht auswerten.\n"

#: ../../deluser:403
#, c-format
msgid "Unknown variable `%s' at %s:%s.\n"
msgstr "Unbekannte Variable `%s' in %s:%s.\n"

msgid "Only root may remove a user or group from the system.\n"
msgstr "Nur root darf Benutzer oder Gruppen vom System löschen.\n"
