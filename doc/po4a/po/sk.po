#: ../deluser.8:123
#, no-wrap
msgid "B<--backup>"
msgstr ""

#. type: Plain text
#: ../deluser.8:127
msgid ""
"Backup all files contained in the userhome and the mailspool-file to a file "
"named /$user.tar.bz2 or /$user.tar.gz."
